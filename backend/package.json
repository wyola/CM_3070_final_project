    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch \"<rootDir>/src/tests/unit/**/*.test.ts\"",
    "test:integration": "jest --testMatch \"<rootDir>/src/tests/integration/**/*.test.ts\""
  },
